<article class="widget">
  <div class="weatherIcon"><i class="wi <%=day.icon%>"></i></div>
  <div class="weatherInfo">
    <div class="temperature"><span><%=day.high%>&deg;</span></div>
    <div class="description">    
      <div class="weatherCondition"><%=day.text%></div>    
      <div class="place"></div>
    </div>
  </div>
  <div class="date"><a><%=day.date.split(" ")[0]%><br><span><%=day.date.split(" ")[1]%></span></a></div>
</article>